{
  "name": "shelterfind",
  "version": "1.0.0",
  "engines":{
    "node": "10.15.0"},
  "description": "### Authors     David Noble     Amy Smith     Yasiin Muhammad     Clarissa Rivas",
  "main": "server.js",
  "scripts": {
    "prestart" : "cd app && yarn build",
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'app/*'\" \"yarn client\"",
    "client": "cd app && yarn start",
    "seed": "node scripts/seedDB.js",
    "install": "cd app && yarn",
    "build": "cd app && yarn build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidNoble007/shelterfind.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidNoble007/shelterfind/issues"
  },
  "homepage": "https://github.com/DavidNoble007/shelterfind#readme",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "bcryptjs": "^2.4.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "i": "^0.3.6",
    "jwt-simple": "^0.5.5",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "nodemailer": "^6.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react-bootstrap-sweetalert": "^4.4.1",
    "react-router-dom": "^5.0.0",
    "react-slack-chat": "^1.10.2",
    "redirect": "^0.2.0"
  }
}
